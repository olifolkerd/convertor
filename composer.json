{
    "name": "olifolkerd/convertor",
    "description": "An easy to use PHP unit conversion library.",
    "license": "MIT",
    "authors": [
        {
            "name": "Oli Folkerd",
            "email": "oliver.folkerd@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Olifolkerd\\Convertor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olifolkerd\\Convertor\\Tests": "tests/"
        }
    },
    "minimum-stability": "dev"
}
